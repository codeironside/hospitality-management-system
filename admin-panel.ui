       <property name="styleSheet">
        <string notr="true">/* QTabWidget Base Style */
QTabWidget::pane {

    border: 1px solid #d8b89a; /* Slight border around content area */
    border-radius: 8px; /* Rounded corners */
    padding: 10px;
}

/* Tab Bar (tab buttons area) */
QTabBar::tab {
  
    color: #402A12; /* Deep brown text color */
    padding: 10px 20px; /* Padding inside the tab buttons */
    border: 1px solid #d8b89a; /* Border around each tab */
    border-radius: 10px 10px 0 0; /* Top corners rounded */
    margin-right: 2px; /* Small gap between tabs */
    min-width: 100px; /* Minimum width for each tab */
    font-size: 14px;
    font-family: 'Roboto', sans-serif; /* Clean font */
    transition: background-color 0.3s ease, color 0.3s ease; /* Smooth hover transition */
}

/* Hover Effect on Tabs */
QTabBar::tab:hover {
    background-color: #f7e2cc; /* Light orange on hover */
    color: #402A12; /* Keep text color the same on hover */
}

/* Active Tab (selected) */
QTabBar::tab:selected {
    background-color: #402A12; /* Deep brown for the active tab */
    color: #fff; /* White text for the active tab */
    border: 1px solid #d8b89a; /* Keep border consistent */
    padding-bottom: 2px; /* Subtle padding change to indicate active state */
}

/* Disabled Tab */
QTabBar::tab:disabled {
    background-color: #e0e0e0; /* Gray background for disabled tabs */
    color: #9e9e9e; /* Gray text for disabled tabs */
    border: 1px solid #d8b89a;
}

/* Styling the Tab Bar (the line the tabs sit on) */
QTabBar {
    background-color: #ebe6e0; /* Same as tab background */
    border-bottom: 1px solid #d8b89a; /* Border under the tabs */
}

/* Scroll buttons for tab navigation */
QTabBar::scroller {
    width: 15px;
    height: 15px;
    background-color: #ebe6e0;
}

/* Close button on tabs (if applicable) */
QTabBar::close-button {
    image: url(':/icons/close.png'); /* Custom close icon if needed */
    margin: 3px; /* Small margin around close button */
}
QTabBar::close-button:hover {
    image: url(':/icons/close-hover.png'); /* Hover effect for close button */
}

/* Tab Widget Frame (surrounding area) */
QTabWidget::tab-bar {
    alignment: center; /* Align tabs in the center */
    background-color: #ebe6e0;
}

/* Scrollable Tab Widget */
QTabBar::scroller {
    border: none;
}
</string>
