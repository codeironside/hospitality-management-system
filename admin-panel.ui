        <property name="styleSheet">
         <string notr="true">/* Base Style for ComboBox */
QComboBox {
    background-color: #ffffff;  /* White background */
    border: 2px solid #c4c4c4;  /* Light gray border */
    border-radius: 8px;  /* Rounded corners */
    padding: 8px 12px;  /* Padding for the combo box */
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
    color: #402A12;  /* Deep brown text color */
    box-shadow: 0px 4px 8px rgba(0, 0, 0, 0.15);  /* Soft shadow for depth */
}

/* Styling the Arrow Button */
QComboBox::drop-down {
    border-left: 2px solid #c4c4c4;  /* Separating line for dropdown arrow */
    background-color: #f7f2ed;  /* Light background for the dropdown area */
    width: 30px;  /* Width of the dropdown arrow */
    border-top-right-radius: 8px;  /* Rounded top-right corner */
    border-bottom-right-radius: 8px;  /* Rounded bottom-right corner */
}

QComboBox::down-arrow {
    image: url(path_to_your_icon);  /* Custom dropdown arrow image */
    width: 10px;  /* Arrow size */
    height: 10px;
}

/* Hover Effect */
QComboBox:hover {
    border: 2px solid #775d41;  /* Slightly darker border on hover */
    background-color: #f7f2ed;  /* Subtle change in background color */
    box-shadow: 0px 6px 12px rgba(0, 0, 0, 0.2);  /* Stronger shadow on hover */
}

/* Focus Effect */
QComboBox:focus {
    border: 2px solid #402A12;  /* Deep brown border when focused */
    background-color: #ffffff;  /* Keep background white */
    box-shadow: 0px 6px 12px rgba(0, 0, 0, 0.3);  /* Intense shadow on focus */
}

/* Dropdown Menu Styling */
QComboBox QAbstractItemView {
    background-color: #ffffff;  /* White background for dropdown */
    border: 2px solid #c4c4c4;  /* Light border around the dropdown */
    border-radius: 8px;  /* Rounded corners for the dropdown list */
    padding: 4px 0;  /* Padding inside the dropdown */
    color: #402A12;  /* Text color */
    selection-background-color: #f7e2cc;  /* Highlight color for selected item */
    selection-color: #402A12;  /* Text color for selected item */
    box-shadow: 0px 8px 16px rgba(0, 0, 0, 0.2);  /* Shadow for dropdown */
}

/* Hover Effect for Items in Dropdown */
QComboBox QAbstractItemView::item:hover {
    background-color: #f7e2cc;  /* Light orange hover effect for dropdown items */
}

/* Disabled ComboBox */
QComboBox:disabled {
    background-color: #e0e0e0;  /* Light gray background when disabled */
    color: #9e9e9e;  /* Gray text for disabled state */
    border: 2px solid #9e9e9e;  /* Gray border for disabled state */
}

/* Arrow Button Hover Effect */
QComboBox::drop-down:hover {
    background-color: #e6e6e6;  /* Slightly darker gray for the dropdown button on hover */
}
</string>
