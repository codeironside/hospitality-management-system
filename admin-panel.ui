          <string notr="true">/* QTableWidget/QTableView Base Style */
QTableWidget, QTableView {
    border: 1px solid #9A7C65; /* Lighter border */
    border-radius: 8px; /* Rounded corners */
    background-color: #f5e7d9; /* Light beige background */
    gridline-color: transparent; /* No gridlines for columns */
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
}

/* Header Style (Flat, No Raised Effect) */
QHeaderView::section {
    background-color: #D6B89A; /* Lighter brown variation for the header */
    color: #604731; /* Soft brown text */
    padding: 8px;
    border: none; /* No borders for header */
    font-weight: bold;
    font-size: 14px;
    text-align: left;
    transition: background-color 0.3s ease, color 0.3s ease;
}

/* Hover Effect on Header */
QHeaderView::section:hover {
    background-color: #B08A6A; /* Slightly darker on hover */
    color: #402A12; /* Darker text on hover */
}

/* Alternating Row Colors with Lighter Shades */
QTableWidget::item, QTableView::item {
    background-color: #fff; /* Default row background */
    border: none; /* No column borders */
    border-bottom: 1px solid #D6B89A; /* Lighter bottom border */
}

QTableWidget::item:nth-child(even), QTableView::item:nth-child(even) {
    background-color: #f7e2cc; /* Light beige for alternate rows */
    border-bottom: 1px solid #9A7C65; /* Slightly darker bottom border for even rows */
}

QTableWidget::item:hover, QTableView::item:hover {
    background-color: #E9D4C2; /* Light brown background on row hover */
    color: #402A12; /* Soft dark brown text on hover */
}

/* First Column (Serial Number or Fixed Column) */
QTableWidget::item:first-child, QTableView::item:first-child {
    background-color: #f2dec6; /* Slightly different background for first column */
    font-weight: bold;
    text-align: center;
}

/* Slim Borders Around Rows (Alternate Colors) */
QTableWidget::item, QTableView::item {
    border-bottom: 1px solid #D6B89A; /* Lighter bottom border for odd rows */
}

QTableWidget::item:nth-child(even), QTableView::item:nth-child(even) {
    border-bottom: 1px solid #9A7C65; /* Slightly darker border for even rows */
}

/* Scrollbar Style */
QScrollBar:vertical {
    border: none;
    background-color: #f7e2cc; /* Same as table background */
    width: 12px;
}

QScrollBar::handle:vertical {
    background-color: #9A7C65; /* Lighter brown for scrollbar handle */
    min-height: 20px;
    border-radius: 6px;
}

QScrollBar::handle:vertical:hover {
    background-color: #D6B89A; /* Lighter hover effect */
}

QScrollBar::sub-line, QScrollBar::add-line {
    border: none;
    background: none;
}
