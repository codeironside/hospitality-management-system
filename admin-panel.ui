        <property name="styleSheet">
         <string notr="true">/* Base Style for the Next Button */
QPushButton {
    background-color: #ebe6e0;  /* Light base color */
    border: 2px solid #c4c4c4;  /* Light gray border */
    border-radius: 10px;  /* Rounded corners */
    padding: 10px 20px;  /* Padding for the button */
    color: #402A12;  /* Deep brown text color */
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
    font-weight: bold;
    box-shadow: 0px 4px 6px rgba(0, 0, 0, 0.1);  /* Soft shadow */
    transition: background-color 0.3s ease, border 0.3s ease, box-shadow 0.3s ease;  /* Smooth transitions */
}

/* Hover Effect */
QPushButton:hover {
    background-color: #f7e2cc;  /* Light orange background on hover */
    border: 2px solid #402A12;  /* Deep brown border on hover */
    box-shadow: 0px 6px 8px rgba(0, 0, 0, 0.2);  /* Slightly stronger shadow */
}

/* Pressed (Clicked) Effect */
QPushButton:pressed {
    background-color: #d8b89a;  /* Darker orange when clicked */
    border: 2px solid #775d41;  /* Slightly darker brown border on click */
    box-shadow: 0px 2px 4px rgba(0, 0, 0, 0.1);  /* Reduced shadow on click */
}

/* Disabled State */
QPushButton:disabled {
    background-color: #e0e0e0;  /* Light gray background when disabled */
    color: #9e9e9e;  /* Gray text for disabled state */
    border: 2px solid #c4c4c4;  /* Keep the border light */
    box-shadow: none;  /* No shadow when disabled */
}
