    border: none; /* No borders for header */
    padding: 0px;
    margin: 0px;
}

/* Table cells padding */
QTableWidget::item {
    padding: 10px; /* Padding inside table cells */
    border: none; /* No borders between table cells */
}

/* Row hover effect */
QTableWidget::item:hover {
    background-color: #f1f1f1; /* Slight hover effect */
}

/* Selected row style */
QTableWidget::item:selected {
    background-color: #e0f7fa;
    color: #424242;
}

/* Scrollbar Styling */
