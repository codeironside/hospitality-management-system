}

/* Header Style: Flat, with a slight background color */
QHeaderView::section {
    background-color: #eeeeee; /* Light gray for headers */
    color: #424242; /* Dark gray text */
    font-weight: bold;
    border: none; /* No border around headers */
    padding: 8px; /* Padding to make it spacious */
    text-align: left; /* Align text to the left */
    height: 30px; /* Adjust height for headers */
}

/* Flatten the header by removing any raised effect */
