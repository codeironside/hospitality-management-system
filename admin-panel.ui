        <property name="styleSheet">
         <string notr="true">/* Base Style for the Line Edit */
QLineEdit {
    background-color: #ffffff;  /* White background */
    border: 2px solid #c4c4c4;  /* Light gray border */
    border-radius: 8px;  /* Rounded corners */
    padding: 8px 12px;  /* Padding inside the input */
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
    color: #402A12;  /* Deep brown text */
}

/* Placeholder Text Styling */
QLineEdit[placeholderText] {
    color: #9e9e9e;  /* Light gray for placeholder text */
}

/* Hover Effect */
QLineEdit:hover {
    border: 2px solid #775d41;  /* Slightly darker brown border on hover */
    background-color: #f7f2ed;  /* Slightly off-white background on hover */
}

/* Focus Effect */
QLineEdit:focus {
    border: 2px solid #402A12;  /* Deep brown border when focused */
    background-color: #ffffff;  /* Ensure background remains white */
    outline: none;  /* Remove any focus outline */
}

/* Style for the Clear Button (X) */
QLineEdit::clear-button {
    image: url(path_to_clear_icon);  /* Set an image for the clear button */
    subcontrol-position: right;  /* Position clear button to the right */
