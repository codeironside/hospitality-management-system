QTableWidget {
    border: none;
    gridline-color: #d6d6d6; /* Slim light gray gridline */
    background-color: #ffffff; /* White background */
    alternate-background-color: #f9f9f9; /* Light alternate row background */
    selection-background-color: #e0f7fa; /* Light blue background for selected row */
    selection-color: #000000; /* Dark text for selected row */
    font-family: 'Roboto', sans-serif;
    font-size: 14px;
}

/* Alternate rows with light gray and white background */
QTableWidget::item {
    border-bottom: 1px solid #d6d6d6; /* Slim border between rows */
    padding: 10px;
}

/* Alternate background for even rows */
QTableWidget::item:alternate {
    background-color: #f9f9f9; /* Slightly different color for alternating rows */
}

/* Hover effect for rows */
QTableWidget::item:hover {
    background-color: #f1f1f1; /* Light hover effect on row */
}

/* Selected row */
QTableWidget::item:selected {
    background-color: #e0f7fa;
    color: #424242;
}

/* Flat header style */
QHeaderView::section {
    background-color: #eeeeee; /* Light gray header background */
    color: #424242; /* Dark text */
    font-weight: bold;
    border: none;
    padding: 8px;
    text-align: left;
    height: 30px;
    transition: background-color 0.3s ease; /* Smooth transition for hover */
}

/* Header hover effect */
QHeaderView::section:hover {
    background-color: #d4d4d4; /* Darker gray on hover */
}

/* Flatten header */
QHeaderView {
    border: none;
}

