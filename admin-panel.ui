QScrollBar:vertical {
    background-color: #f5f5f5;
    width: 12px;
}

QScrollBar::handle:vertical {
    background-color: #bdbdbd;
    min-height: 20px;
}

QScrollBar:horizontal {
    background-color: #f5f5f5;
    height: 12px;
}

QScrollBar::handle:horizontal {
    background-color: #bdbdbd;
    min-width: 20px;
}

/* Slim borders for gridlines */
QTableWidget::item {
    border: 1px solid #d6d6d6; /* Slim light gray borders between cells */
}
